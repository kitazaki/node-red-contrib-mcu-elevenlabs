{
        "include": [
                "$(MODDABLE)/examples/manifest_base.json",
		"$(MODDABLE)/modules/io/manifest.json",
                "./manifest_elevenlabsstreamer.json"
        ],
	"modules": {
		"*": [
			"./elevenlabs"
                ],
		"pins/*": [
			"$(MODULES)/pins/i2s/*"
                ]
	},
	"defines": {
                "audioOut": {
                        "bitsPerSample": 16,
                        "numChannels": 1,
                        "sampleRate": 44100,
                        "queueLength": 12
                }
        },
        "config": {
                "startupSound": false
        },
	"preload": [
                "elevenlabs"
        ]
}
